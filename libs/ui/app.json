{
  "expo": {
    "name": "NOX UI",
    "slug": "ui",
    "scheme": "ui",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*", "assets/**"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nox-technologies.ui"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nox_technologies.ui"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "8b12c7ed-1bd1-41dc-a097-f631f07a5b1a"
      }
    }
  }
}
